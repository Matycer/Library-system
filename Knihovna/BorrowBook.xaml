        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="BorrowBook" Height="345.221" Width="320.956" Loaded="Window_Loaded">
    <Grid>
        <TextBlock x:Name="txtBook" HorizontalAlignment="Left" Margin="10,27,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="18"/>
        <TextBlock x:Name="txtAuthor" HorizontalAlignment="Left" Margin="10,73,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbUsers" HorizontalAlignment="Left" Margin="10,145,0,0" DisplayMemberPath="Name" SelectedValuePath="Id" VerticalAlignment="Top" Width="216"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,124,0,0" TextWrapping="Wrap" Text="Uživatel" VerticalAlignment="Top"/>
        <Button x:Name="bntVypujcit" Content="Vypůjčit" HorizontalAlignment="Left" Margin="214,273,0,0" VerticalAlignment="Top" Width="76" Click="bntVypujcit_Click"/>

    </Grid>
</Window>
